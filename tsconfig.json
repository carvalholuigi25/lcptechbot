{
    "compilerOptions": {
        "lib": ["ESNext"],
        "target": "ES2022",
        "module": "CommonJS",
        "esModuleInterop": false,
        "strict": true,
        "alwaysStrict": false,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    },
    "include": ["**/*"],
    "exclude": ["node_modules"]
}